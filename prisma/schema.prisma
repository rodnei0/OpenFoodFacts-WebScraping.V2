// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Products {
  code          BigInt   @id
  barcode       String   
  imported_t    DateTime
  url           String
  product_name  String
  quantity      String
  categories    String
  packaging     String
  brands        String
  image_url     String
}